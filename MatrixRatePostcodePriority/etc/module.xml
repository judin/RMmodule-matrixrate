<?xml version="1.0"?>
<!--
/**
 * MatrixRate Postcode Priority Extension
 *
 * Ensures more specific postcode patterns take priority over less specific ones.
 * For example, NP% will be prioritized over N% when matching a postcode starting with NP.
 */
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Module/etc/module.xsd">
    <module name="WebShopApps_MatrixRatePostcodePriority">
        <sequence>
            <module name="WebShopApps_MatrixRate"/>
        </sequence>
    </module>
</config>
